            <kind>1</kind>
            <string>///Movimiento y COLISIONES del personaje

// Obtener las entradas de teclado 

key_right = keyboard_check(vk_right);
key_left = keyboard_check(vk_left);
key_z = keyboard_check_pressed(vk_up);

//Operaciones con las entradas de teclado
move = key_right - key_left;
hsp = move * movespeed;


if (vsp &lt; 10) {
    vsp += grav;
}

if (place_meeting(x, y+1, objWall) or place_meeting(x, y+1, objHierroH)){
    vsp = key_z * -jumpspeed;
    doblesalto = 1;
    
}else if (doblesalto == 1 &amp;&amp; key_z&gt;0){
    vsp = 1 * -jumpspeed;
    doblesalto = 0;
}


//Colision horizontal 
if (place_meeting(x+hsp, y, objWall)){
    while (!place_meeting(x+sign(hsp), y, objWall)) {
        x+= sign(hsp);
    }
    hsp=0;
}
if (place_meeting(x+hsp, y, objHierroH)){
    while (!place_meeting(x+sign(hsp), y, objHierroH)) {
        x+= sign(hsp);
    }
    hsp=objHierroH.hsp;
}
x += hsp;


//Colision Vertical
if (place_meeting(x, y+vsp, objWall)  ){
    
    while (!place_meeting(x, y+sign(vsp), objWall)) {
        y+= sign(vsp);
    }
    
    vsp=0;
}
if (place_meeting(x, y+vsp, objHierroH)  ){
    
    while (!place_meeting(x, y+sign(vsp), objHierroH)) {
        y+= sign(vsp);
    }
    vsp=0;
}


y += vsp;







