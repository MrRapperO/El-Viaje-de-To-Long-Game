            <string>///Interaccion con objetos 

//Puertas

if (place_meeting(x, y, objPuerta) and keyboard_check_pressed(vk_down)){
    
    with (instance_nearest(x, y, objPuerta)){
        nombre = (object_get_name(object_index));
        nivel = string_char_at(nombre, 10);
        global.subnivel = int64(nivel);
        if (int64(nivel)!=0){
            global.posx = x;
            global.posy = y;
        }
        room_goto(((global.nivel-1)*3)+int64(nivel)+1);
        
    }

}

//botones


if (place_meeting(x, y, objBoton)){
    if (keyboard_check_pressed(vk_down)){ 
            if(global.interruptores[global.subnivel-1]==false){
                objBoton.sprite_index = sprBotonTrue;
                global.interruptores[global.subnivel-1] = true;
            }else {
                objBoton.sprite_index = sprBotonFalse;
                global.interruptores[global.subnivel-1] = false;
            }
    
    }
}


//Zanahoria

if (place_meeting(x, y, objZanahoria)){
    global.zanahorias += 1;
    with (instance_nearest(x, y, objZanahoria)){
        instance_destroy();
    }
    audio_play_sound(sndMordisco, 6,false);
}

//Meta
if (place_meeting(x,y,objMeta) ){
    global.posx = 0;
    global.posy = 0;
    room_goto(1+(global.subnivel+1)*4);
}


    score = global.zanahorias;

