          <argument>
            <kind>1</kind>
            <string>///CAMBIO DE Sprites

//direccion == 1 == derecha || direccion == 2 == izquierda
// estadoDeSalto : 0 = en suelo : 1= saltando : 2 = cayendo 


//encuentra la direccion
if (keyboard_check(vk_right)){
    direccion  = 1;    
}else if(keyboard_check(vk_left)){
    direccion = 2;
}

if (keyboard_check(ord('X')) || keyboard_check(ord('S')) || keyboard_check(ord('Z'))){
    enAnim =true;
}

//encuentra el estado de zool en el aire
if (vsp&lt;0){
    estadoDeSalto = 1;
}else {
    if (place_meeting(x, y+1, objWall)){
        estadoDeSalto = 0;
    }else if (place_meeting(x, y+1, objHierroH)){
        estadoDeSalto = 0;
    }else {
        estadoDeSalto = 2;
    }
}

if (enAnim==false){


if (estadoDeSalto==0){
    if (keyboard_check(vk_right) || keyboard_check(vk_left)){
        image_speed = 0.5;
        sprite_index = sprZoolWalkRight;
        if (direccion!=1){
            image_xscale = -1;
        }else {
            image_xscale = 1;
        }
    }else if (keyboard_check(ord('A'))){
        image_speed = 0.25;
        
        alarm[3]=1;
        sprite_index = sprZoolDance;
        if (direccion!=1){
            image_xscale = -1;
        }else {
            image_xscale = 1;
        }
    }else {
        sprite_index = sprZoolStandRight;
        if (direccion!=1){
            image_xscale = -1;
        }else {
            image_xscale = 1;
        }
    }
    image_angle = 0;
}else if (estadoDeSalto==1){
    if (doblesalto!=1){
        image_speed = 0.5;
        sprite_index = sprZoolDobleJumpRight;
        
        
        
    }else {
        sprite_index = sprZoolJumpRight;
        if (direccion!=1){
            image_xscale = -1;
        }else {
            image_xscale = 1;
        }
        
        
    }
}else if (estadoDeSalto==2){
    sprite_index = sprZoolFallRight;
    if (direccion!=1){
        image_xscale = -1;
    }else {
        image_xscale = 1;
    }
    image_angle = 0;
}

}else if (keyboard_check(ord('X')) || keyboard_check(ord('S')) || keyboard_check(ord('Z'))){
    image_speed = 0.5;
    image_angle = 0;
    sprite_index = sprZoolAtacar;
    if (direccion!=1){
        image_xscale = -1;
    }else {
        image_xscale = 1;
    }
    
    alarm[1]=8;
}





